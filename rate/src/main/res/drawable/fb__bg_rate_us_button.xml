<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#33FFFFFF">
    <item>
        <selector>
            <item android:state_enabled="true">
                <shape>
                    <solid android:color="@color/fb_color_button_submit_enable" />
                    <corners android:radius="@dimen/fb_radius_submit_size"/>
                </shape>
            </item>

            <item android:state_enabled="false">
                <shape>
                    <solid android:color="@color/fb_color_button_submit_disable" />
                    <corners android:radius="@dimen/fb_radius_submit_size"/>
                </shape>
            </item>
        </selector>
    </item>
</ripple>